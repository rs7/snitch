[
  {likes, [
    {worker_count, 2}
  ]},
  {lager, [
    {log_root, "./log"},
    {handlers, [
      {lager_console_backend, [
        info, {lager_default_formatter, [time," [",severity,"] ", module, "-", pid, " ", message, "\n"]}
      ]},
      {lager_file_backend, [{file, "info.log"}, {level, info}]},
      {lager_file_backend, [{file, "warning.log"}, {level, warning}]}
    ]}
  ]}
].
